<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    </PropertyGroup>

    <Import Project="..\Props\target.props" />

    <ItemGroup>
        <ProjectReference Include="..\CSharpSyntaxGenerator\CSharpSyntaxGenerator.csproj">
            <OutputItemType>Analyzer</OutputItemType>
            <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
            <SetTargetFramework>TargetFramework=netstandard2.0</SetTargetFramework>
        </ProjectReference>
        
        <PackageReference Include="Antlr4.CodeGenerator" Version="4.6.6">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
        </PackageReference>

        <PackageReference Include="Antlr4.Runtime" Version="4.6.6" />
        <PackageReference Include="Aquila.Microsoft.CodeAnalysis" Version="4.1.0-dev">
            <SetTargetFramework>TargetFramework=netstandard2.0</SetTargetFramework>
        </PackageReference>
        <PackageReference Include="McMaster.Extensions.CommandLineUtils" Version="2.2.4" />
        <PackageReference Include="morelinq" Version="3.3.2" />
        <PackageReference Include="System.ServiceModel.Primitives" Version="4.7.0" />
    </ItemGroup>

    <ItemGroup>
        <Content Include="Syntax\Parser\ZSharpLexer.g4" />
        <Content Include="Syntax\Parser\ZSharpParser.g4" />
        <Content Include="Syntax\Parser\ZSharpPreprocessorParser.g4" />
    </ItemGroup>

    <ItemGroup>
        <Compile Update="AquilaResources.Designer.cs">
            <AutoGen>True</AutoGen>
            <DependentUpon>AquilaResources.resx</DependentUpon>
            <DesignTime>True</DesignTime>
        </Compile>
        <Compile Update="Errors\ErrorStrings.Designer.cs">
            <AutoGen>True</AutoGen>
            <DependentUpon>ErrorStrings.resx</DependentUpon>
            <DesignTime>True</DesignTime>
        </Compile>
    </ItemGroup>
    <ItemGroup>
        <InternalsVisibleTo Include="Aquila.Compiler.Test2, " />
        <InternalsVisibleTo Include="Aquila.CodeAnalysis.Tests, " />
    </ItemGroup>
    <ItemGroup>
        <Compile Update="AquilaResources.Designer.cs">
            <DesignTime>True</DesignTime>
            <AutoGen>False</AutoGen>
            <DependentUpon>AquilaResources.resx</DependentUpon>
        </Compile>
    </ItemGroup>

    <ItemGroup>

        <EmbeddedResource Update="AquilaResources.resx">
            <Generator>ResXFileCodeGenerator</Generator>
            <LastGenOutput>AquilaResources.Designer.cs</LastGenOutput>
        </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Update="Errors\ErrorStrings.resx">
            <Generator>ResXFileCodeGenerator</Generator>
            <LastGenOutput>ErrorStrings.Designer.cs</LastGenOutput>
        </EmbeddedResource>

    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\Aquila.Metadata\Aquila.Metadata.csproj" />
        <ProjectReference Include="..\Aquila.Querying\Aquila.Querying.csproj" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Syntax2" />
      <Folder Include="Syntax2\Syntax\InternalSyntax" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="test.txt" />
    </ItemGroup>

</Project>